model TipoCambio {
  id         Int      @id @default(autoincrement())
  fecha      DateTime
  moneda     String
  tasaCompra Float
  tasaVenta  Float

  usuarioCreador      Int?
  usuarioActualizador Int?
  createdAt           DateTime  @default(now())
  updatedAt           DateTime  @default(now()) @updatedAt
  deletedAt           DateTime?

  @@map("tipo_cambio")
}
