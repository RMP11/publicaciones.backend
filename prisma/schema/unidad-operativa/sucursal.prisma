model Sucursal {
  id          Int      @id @default(autoincrement())
  descripcion String
  esTienda    Boolean?

  usuarioCreador      Int?
  usuarioActualizador Int?
  createdAt           DateTime           @default(now())
  updatedAt           DateTime           @default(now()) @updatedAt
  deletedAt           DateTime?
  ProductoSucursal    ProductoSucursal[]
  Venta               Venta[]

  @@map("sucursales")
}
