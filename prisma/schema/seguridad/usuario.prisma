model Usuario {
  id            Int          @id @default(autoincrement())
  correo        String       @unique
  contrasena    String
  nombre        String
  usuariosRoles UsuarioRol[]

  createdAt DateTime  @default(now()) @map("created_at")
  updatedAt DateTime  @updatedAt @map("updated_at")
  deletedAt DateTime? @map("deleted_at")

  @@map("usuarios")
}
